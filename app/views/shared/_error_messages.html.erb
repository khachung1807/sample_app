<% if object.errors.any? %>
  <div id = "error_explanation">
    <div class="alert alert-danger">
      <%= t("error_details") %><%= pluralize(object.errors.count, t("error")) %>
    </div>
    <ul>
      <% onject.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>
