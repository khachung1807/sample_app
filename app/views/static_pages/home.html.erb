<% provide :title, t(".title") %>
<body>
  <div class="center jumbotron">
    <h1><%= t ".title" %></h1>
    <p>
      <%= link_to (t ".linkToHome"), "http://www.railstutorial.org/" %><%= t ".linkName" %>
    </p>
    <%= link_to (t ".linkHelp"), help_path %> |
    <%= link_to (t ".linkAbout"), about_path %> |
    <%= link_to (t ".linkContact"), contact_path %><br>
    <%= link_to (t "signup"), "#" , class: "btn btn-lg btn-primary" %>
  </div>
  <%= link_to image_tag("kitten.jpg", alt: "Kitten Image"), "http://rubyonrails.org/" %><br>
  <%= link_to image_tag("rails.png", alt: "Rails logo"), "http://rubyonrails.org/" %><br>
</body>
